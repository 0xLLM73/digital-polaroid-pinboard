# Browser Configuration
HEADLESS=true
VIEWPORT_WIDTH=1200
VIEWPORT_HEIGHT=800
SCREENSHOT_DPI=144

# Monitoring
HEALTH_CHECK_INTERVAL=300000
ERROR_ALERT_THRESHOLD=5 